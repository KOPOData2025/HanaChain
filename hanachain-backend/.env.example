# Application
SPRING_PROFILES_ACTIVE=prod
SERVER_PORT=8080

# Database
SPRING_DATASOURCE_URL=jdbc:oracle:thin:@localhost:1521/XEPDB1
SPRING_DATASOURCE_USERNAME=hanachain
SPRING_DATASOURCE_PASSWORD=your_db_password
SPRING_JPA_HIBERNATE_DDL_AUTO=validate

# Flyway
SPRING_FLYWAY_ENABLED=true
SPRING_FLYWAY_BASELINE_ON_MIGRATE=true
SPRING_FLYWAY_LOCATIONS=classpath:db/migration

# FDS Service
FDS_SERVICE_URL=http://fds-service:8000
FDS_SERVICE_TIMEOUT=30000

# Blockchain
ALCHEMY_API_KEY=your_alchemy_api_key
ETHEREUM_NETWORK=sepolia
ETHEREUM_CHAIN_ID=11155111

# JWT
JWT_SECRET=your_jwt_secret_key_min_256_bits
JWT_EXPIRATION=86400000

# AWS (for deployment script)
AWS_REGION=ap-northeast-2
AWS_SECRET_NAME=hanachain/prod/backend

# Logging
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_HANACHAIN=DEBUG
